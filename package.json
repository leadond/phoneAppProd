{
  "name": "phone-range-nexus",
  "private": true,
  "version": "1.0.0",
  "description": "Phone Range Nexus - Local Database Edition for corporate firewall environments",
  "type": "module",
  "scripts": {
    "dev": "concurrently -k -n FRONTEND,BACKEND -c cyan,magenta \"vite\" \"tsx server.cjs\"",
    "dev:frontend": "vite",
    "dev:all": "concurrently -k -n FRONTEND,BACKEND -c cyan,magenta \"npm run dev\" \"python3 /Users/derrickleadon/Downloads/business-phone-number-management-app/api/start.py\"",
    "build": "vite build",
    "build:dev": "vite build --mode development",
    "build:prod": "vite build --mode production",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "jest",
    "preview": "vite preview",
    "preview:prod": "vite preview --mode production",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:report": "playwright show-report",
    "security:audit": "npm audit --audit-level high",
    "security:fix": "npm audit fix",
    "security:headers": "curl -I http://localhost:8080 | grep -E '(X-Frame-Options|X-Content-Type-Options|X-XSS-Protection|Strict-Transport-Security)'",
    "start": "tsx server.cjs",
    "start:production": "NODE_ENV=production tsx server.cjs",
    "build:production": "npm run lint && npm run security:audit && vite build --mode production",
    "deploy": "bash scripts/deploy.sh",
    "backup": "bash scripts/backup-database.sh",
    "maintenance": "bash scripts/maintenance.sh",
    "migrate": "node scripts/migrate-database.js",
    "clean": "rm -rf dist/ node_modules/.vite/",
    "clean:all": "rm -rf dist/ node_modules/ package-lock.json && npm install"
  },
  "keywords": [
    "phone-management",
    "number-ranges",
    "pbx-integration",
    "local-database",
    "corporate-firewall",
    "offline-capable"
  ],
  "author": "Phone Range Nexus Team",
  "license": "MIT",
